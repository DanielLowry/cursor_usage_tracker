{
  "name": "@cursor-usage/db",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rimraf .turbo node_modules dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate",
    "lint": "eslint .",
    "test": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0"
  },
  "devDependencies": {
    "@cursor-usage/config": "workspace:*",
    "@cursor-usage/types": "workspace:*",
    "eslint": "^8.56.0",
    "prisma": "^5.8.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.0"
  }
}
