{
  "manifest_version": 3,
  "name": "Cursor Session Helper",
  "version": "1.0",
  "description": "Securely captures Cursor session data for authorized administrators",
  "permissions": [
    "cookies",
    "storage",
    "scripting"
  ],
  "host_permissions": [
  "https://cursor.com/*",
  "https://*.cursor.com/*"
  ],
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "src/background.js",
    "type": "module"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
